#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/assemblyscript@0.28.3/node_modules/assemblyscript/bin/node_modules:/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/assemblyscript@0.28.3/node_modules/assemblyscript/node_modules:/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/assemblyscript@0.28.3/node_modules:/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/assemblyscript@0.28.3/node_modules/assemblyscript/bin/node_modules:/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/assemblyscript@0.28.3/node_modules/assemblyscript/node_modules:/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/assemblyscript@0.28.3/node_modules:/Users/a77/Documents/fanny/学习/demo/asc-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/asinit.js" "$@"
else
  exec node  "$basedir/../../bin/asinit.js" "$@"
fi
